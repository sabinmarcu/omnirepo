{
  "name": "@sabinmarcu/team-rotation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.16.5",
    "@mui/material": "^5.16.5",
    "@mui/x-date-pickers": "^7.12.0",
    "@sabinmarcu/jotai-storage-adapter-querystring": "workspace:*",
    "@sabinmarcu/moving-mesh-background": "workspace:*",
    "@sabinmarcu/mui-material-theme": "workspace:*",
    "@sabinmarcu/use-match-media": "workspace:*",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.12",
    "jotai": "^2.9.1",
    "jotai-devtools": "^0.10.0",
    "jotai-effect": "^1.0.0",
    "jotai-optics": "^0.4.0",
    "nanoid": "^5.0.7",
    "optics-ts": "^2.4.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "5.5.4",
    "vite": "^5.3.4"
  },
  "tscmono": {
    "preset": "viteApp"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./src/*": {
      "import": "./src/*",
      "default": "./src/*"
    },
    "./*": {
      "import": "./dist/*.js",
      "default": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typeVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  }
}
