{
  "name": "@sabinmarcu/eslint-config",
  "version": "1.0.1",
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-airbnb": "^18.0.4",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-expect-type": "^0.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-modules-newlines": "^0.0.7",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-unicorn": "^45.0.2",
    "glob": "^8.0.3",
    "ts-deepmerge": "^5.0.0"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*",
    "@types/eslint": "^8.4.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.cjs"
    },
    "./src/*": {
      "import": "./src/*",
      "require": "./src/*"
    },
    "./*": {
      "import": "./esm/*.mjs",
      "require": "./cjs/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "tscmono": {
    "preset": "lib"
  },
  "typeVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  }
}
