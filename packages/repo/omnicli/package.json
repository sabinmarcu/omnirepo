{
  "name": "@sabinmarcu/omnicli",
  "version": "1.0.1",
  "main": "./cjs/index.cjs",
  "types": "./cjs/index.d.ts",
  "module": "./esm/index.mjs",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.cjs"
    },
    "./src/*": {
      "import": "./src/*",
      "require": "./src/*"
    },
    "./*": {
      "import": "./esm/*.mjs",
      "require": "./cjs/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "bin": {
    "omni": "./bin/omnicli.js"
  },
  "packageManager": "yarn@3.3.0",
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "*",
    "@babel/plugin-proposal-export-namespace-from": "*",
    "@babel/plugin-proposal-nullish-coalescing-operator": "*",
    "@babel/plugin-proposal-optional-chaining": "*",
    "@babel/plugin-syntax-class-properties": "*",
    "@babel/plugin-transform-modules-commonjs": "*",
    "@babel/plugin-transform-typescript": "*",
    "@rnx-kit/babel-plugin-import-path-remapper": "*",
    "@sabinmarcu/utils-fs": "workspace:*",
    "@sabinmarcu/utils-repo": "workspace:*",
    "babel-plugin-dynamic-import-node": "*",
    "babel-plugin-parameter-decorator": "*",
    "clipanion": "^3.2.0-rc.14",
    "glob": "^8.0.3",
    "jiti": "^1.16.1",
    "memfs": "^3.4.12",
    "moize": "^6.1.4",
    "typanion": "^3.12.1",
    "yaml": "^2.2.0"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*",
    "@types/glob": "^8"
  },
  "tscmono": {
    "preset": "lib"
  },
  "typeVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  }
}