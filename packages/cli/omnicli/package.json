{
  "name": "@sabinmarcu/omnicli",
  "version": "1.0.0",
  "main": "./cjs/index.cjs",
  "types": "./cjs/index.d.ts",
  "module": "./esm/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "require": "./cjs/index.cjs",
      "import": "./esm/index.mjs"
    },
    "./*": {
      "require": "./cjs/*.cjs",
      "import": "./esm/*.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "bin": "./bin/omnicli.js",
  "packageManager": "yarn@3.3.0",
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "dependencies": {
    "@sabinmarcu/utils-repo": "workspace:*",
    "clipanion": "^3.2.0-rc.14",
    "glob": "^8.0.3",
    "jiti": "^1.16.0",
    "memfs": "^3.4.12",
    "moize": "^6.1.4",
    "ramda": "^0.28.0",
    "typanion": "^3.12.1",
    "type-fest": "^3.4.0",
    "yaml": "^2.2.0"
  },
  "devDependencies": {
    "@types/glob": "^8",
    "@types/ramda": "^0"
  },
  "tscmono": {
    "preset": "lib"
  }
}
