{
  "name": "@sabinmarcu/commitlint-config-workspaces",
  "version": "1.0.0",
  "main": "./cjs/index.ts",
  "types": "./cjs/index.d.ts",
  "module": "./esm/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./cjs/index.ts",
      "import": "./esm/index.ts"
    },
    "./*": {
      "require": "./cjs/*.ts",
      "import": "./esm/*.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "packageManager": "yarn@3.3.0",
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "preset": "../../.config/build.config.ts"
  },
  "dependencies": {
    "@sabinmarcu/utils-repo": "workspace:*",
    "type-fest": "^3.4.0"
  }
}