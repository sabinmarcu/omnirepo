{
  "name": "@sabinmarcu/eslint-config",
  "version": "2.0.1",
  "dependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@stylistic/eslint-plugin-js": "^2.6.1",
    "@stylistic/eslint-plugin-ts": "^2.6.1",
    "confusing-browser-globals": "^1.0.11",
    "debug": "^4.3.6",
    "eslint-import-resolver-exports": "^1.0.0-beta.5",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-canonical": "^4.18.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "glob": "^8.0.3",
    "globals": "^15.9.0",
    "ts-deepmerge": "^7.0.1"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*",
    "@types/confusing-browser-globals": "^1",
    "@types/debug": "^4",
    "@types/eslint": "^9.6.0",
    "@types/eslint__eslintrc": "^2.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.cjs",
      "types": "./esm/index.d.ts"
    },
    "./src/*": {
      "import": "./src/*",
      "require": "./src/*"
    },
    "./*": {
      "import": "./esm/*.mjs",
      "require": "./cjs/*.cjs",
      "types": "./esm/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "tscmono": {
    "preset": "lib"
  },
  "typeVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "types": "./esm/index.d.ts",
  "peerDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "eslint-plugin-expect-type": "^0.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/eslint-parser": {
      "optional": true
    },
    "eslint-plugin-expect-type": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-storybook": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  }
}
