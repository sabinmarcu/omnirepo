{
  "name": "@sabinmarcu/debug",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./esm/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "main": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src/*": {
      "import": "./src/*",
      "main": "./src/*",
      "default": "./src/*"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "main": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "packageManager": "yarn@3.3.0",
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "dependencies": {
    "@sabinmarcu/config": "workspace:*",
    "@sabinmarcu/observable": "workspace:*",
    "micromatch": "^4.0.5",
    "ramda": "^0.29.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*",
    "@types/micromatch": "^4",
    "@types/ramda": "^0"
  },
  "tscmono": {
    "preset": "lib"
  },
  "typeVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "repository": {
    "url": "git+https://github.com/sabinmarcu/omnirepo.git",
    "type": "git",
    "directory": "workspaces/libs/debug"
  },
  "homepage": "https://sabinmarcu.dev//api/debug"
}
