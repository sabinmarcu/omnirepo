{
  "name": "@sabinmarcu/jotai-storage-adapter-querystring",
  "version": "2.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./esm/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "main": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src/*": {
      "import": "./src/*",
      "main": "./src/*",
      "default": "./src/*"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "main": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "packageManager": "yarn@4.5.1",
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*"
  },
  "tscmono": {
    "preset": "lib"
  },
  "typeVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "peerDependencies": {
    "@types/pako": "*",
    "jotai": "*",
    "pako": "*",
    "zod": "*"
  },
  "repository": {
    "url": "git+https://github.com/sabinmarcu/omnirepo.git",
    "type": "git",
    "directory": "workspaces/libs/jotai-storage-adapter-querystring"
  },
  "homepage": "https:/sabinmarcu.dev/api/jotai-storage-adapter-querystring",
  "dependencies": {
    "js-base64": "^3.7.7"
  }
}
