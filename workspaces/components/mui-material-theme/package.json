{
  "name": "@sabinmarcu/mui-material-theme",
  "version": "1.0.3",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "main": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src/*": {
      "import": "./src/*",
      "main": "./src/*",
      "default": "./src/*"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "main": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "packageManager": "yarn@4.1.1",
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*"
  },
  "tscmono": {
    "preset": "lib",
    "presets": [
      "lib",
      "react"
    ]
  },
  "typeVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "peerDependencies": {
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "react": "*"
  },
  "repository": {
    "url": "git+https://github.com/sabinmarcu/omnirepo.git",
    "type": "git",
    "directory": "workspaces/components/mui-material-theme"
  },
  "homepage": "https:/sabinmarcu.dev/api/mui-material-theme",
  "optionalDependencies": {
    "@mui/icons-material": "^5.16.6",
    "@mui/material": "^5.16.6"
  },
  "peerDependenciesMeta": {
    "@mui/icons-material": {
      "optional": true
    },
    "@mui/material": {
      "optional": true
    }
  },
  "dependencies": {
    "@sabinmarcu/use-local-storage": "workspace:*",
    "@sabinmarcu/use-match-media": "workspace:*"
  }
}
