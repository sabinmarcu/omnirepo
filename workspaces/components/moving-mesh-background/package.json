{
  "name": "@sabinmarcu/moving-mesh-background",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./esm/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "main": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src/*": {
      "import": "./src/*",
      "main": "./src/*",
      "default": "./src/*"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "main": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./esm/*"
      ]
    }
  },
  "packageManager": "yarn@3.3.0",
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "preset": "../../../.config/build.config.ts"
  },
  "devDependencies": {
    "@sabinmarcu/types": "workspace:*",
    "@sabinmarcu/utils-test": "workspace:*"
  },
  "tscmono": {
    "preset": "lib",
    "presets": [
      "lib",
      "react"
    ]
  },
  "typeVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "dependencies": {
    "@sabinmarcu/use-duplicate-ref": "workspace:*",
    "@sabinmarcu/use-prefers-reduced-motion": "workspace:*"
  },
  "peerDependencies": {
    "react": "*"
  },
  "repository": {
    "url": "git+https://github.com/sabinmarcu/omnirepo.git",
    "type": "git",
    "directory": "workspaces/components/moving-mesh-background"
  },
  "homepage": "https://sabinmarcu.dev//api/moving-mesh-background"
}
